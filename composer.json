{
    "name": "saimondev03/form",
    "description": "Advanced form generation for Laravel",
    "type": "library",
    "require": {
        "php": "^7.3 || ^8.0",
        "laravel/framework": "^7.30|^8.0|^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "orchestra/testbench": "5.*",
        "gajus/dindent": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Saimondev03\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Montoya",
            "email": "simonmontoya19@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": ["Saimondev03\\FormServiceProvider"]
        }
    }
}
